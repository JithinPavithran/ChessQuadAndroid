<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.jithin.chessquad.MainActivity">


    <android.support.constraint.ConstraintLayout
            android:layout_margin="0dp"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="H,1:1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/board"
            android:background="@drawable/chess">

        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glx0"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.0"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glx1"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.08333333"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glx2"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.16666666"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glx3"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.25"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glx4"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.333333333"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glx5"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.41666667"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glx6"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.50000000"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glx7"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.58333333"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glx8"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.66666667"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glx9"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.75000000"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glxa"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.83333333"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glxb"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.91666667"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glxc"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="1.0"/>

        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gly0"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.0"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gly1"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.08333333"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gly2"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.16666666"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gly3"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.25"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gly4"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.333333333"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gly5"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.41666667"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gly6"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.50000000"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gly7"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.58333333"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gly8"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.66666667"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gly9"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.75000000"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glya"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.83333333"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glyb"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.91666667"/>
        <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/glyc"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="1.0"/>

    </android.support.constraint.ConstraintLayout>


</android.support.constraint.ConstraintLayout>
